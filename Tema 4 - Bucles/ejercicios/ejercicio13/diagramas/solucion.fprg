    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="numero" type="Integer" array="False" size=""/>
            <declare name="contador" type="Integer" array="False" size=""/>
            <assign variable="contador" expression="0"/>
            <declare name="posi" type="Integer" array="False" size=""/>
            <assign variable="posi" expression="0"/>
            <declare name="nega" type="Integer" array="False" size=""/>
            <assign variable="nega" expression="0"/>
            <while expression="contador&lt;3">
                <input variable="numero"/>
                <if expression="numero&gt;=0">
                    <then>
                        <assign variable="posi" expression="posi+1"/>
                    </then>
                    <else>
                        <assign variable="nega" expression="nega+1"/>
                    </else>
                </if>
                <assign variable="contador" expression="contador+1"/>
            </while>
            <output expression="&quot;En la lista introducida hay &quot; &amp;posi &amp;&quot; numeros positivos&quot;" newline="True"/>
            <output expression="&quot;y &quot; &amp;nega &amp;&quot; numeros negativos&quot;" newline="True"/>
        </body>
    </function>
</flowgorithm>
